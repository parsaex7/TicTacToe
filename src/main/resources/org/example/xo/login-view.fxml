<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.xo.LoginView">
   <children>
      <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="50.0" styleClass="back" stylesheets="@style.css">
         <children>
            <Label stylesheets="@style.css" text="Enter Username">
               <font>
                  <Font name="Goudy Old Style" size="37.0" />
               </font>
            </Label>
            <TextField fx:id="nameTextField" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="46.0" prefWidth="209.0" promptText="name" stylesheets="@style.css">
               <font>
                  <Font name="Goudy Old Style" size="16.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="333.0" prefWidth="600.0" spacing="10.0" styleClass="back" stylesheets="@style.css">
         <children>
            <Label fx:id="warningLable" textFill="RED">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#enterButtonController" styleClass="back" stylesheets="@style.css" text="ENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
